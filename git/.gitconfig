[user]
	email = prayag.s@ahduni.edu.in
	name = PrayagS
    signingKey = F6F31C32CF85286D

[core]
    editor = $EDITOR --cmd 'set colorcolumn=72'

[github]
    user = PrayagS

[init]
    defaultBranch = main

[commit]
    gpgSign = true

[color]
	ui     = auto
	status = auto
	diff   = auto
	branch = auto
	interative = auto

[branch]
	sort = -committerdate

[push]
	default = current

[grep]
	lineNumber = true

[url "git@github.com:"]
	insteadOf = ssh:

[url "https://github.com/"]
	insteadOf = nossh:

[status]
	submoduleSummary = true

[help]
	autocorrect = 10
